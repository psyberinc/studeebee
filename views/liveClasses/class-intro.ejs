<!-- Content -->
<div class="page-content bg-white">
    <!-- inner page banner -->
    <div class="page-banner ovbl-dark"
        style="background-image:url(assets/images/banner/images.jpg);background-size: 100% 200%;">
        <div class="container">
            <div class="page-banner-entry">
                <h1 class="text-white" style="font-weight: 300;">Live Online Class</h1>
            </div>
        </div>
    </div>
    <!-- Breadcrumb row -->
    <div class="breadcrumb-row">
        <div class="container">
            <ul class="list-inline">
                <li><a href="/home">Home</a></li>
                <li>Live Classes</li>
            </ul>
        </div>
    </div>
    <!-- Breadcrumb row END -->
    <!-- contact area -->
    <div class="content-block">


        <!-- Portfolio  -->
        <div class="section-area section-sp1 gallery-bx">
            <div class="container">

                <div class="clearfix">
                    <ul id="masonry" class="ttr-gallery-listing magnific-image row">
                        <li class="action-card col-lg-12 col-md-12 col-sm-12">
                            <div class="event-bx m-b30">
                                <div class="info-bx">

                                    <div class="event-info">
                                        <h4 class="event-title text-center"><a href="#"><%= liveclass.title %> </a>
                                        </h4>
                                        <ul class="media-post text-center">
                                            <li><a href="#"><i class="fa fa-clock-o"></i> <%= liveclass.time1 %> <%= liveclass.time1 %>am</a></li>
                                            <li><a href="#"><i class="fa fa-users"></i> <%= liveclass.level %></a>
                                            </li>
                                        </ul>

                                        <h5 class="text-center">WHO WILL TEACH?</h5>
                                        <p class="text-center"><%= liveclass.instructor %></p>
                                        <p class="text-center">B.Tech Computer Science & Technology, HIET, Ghaziabad</p>

                                        <ul class="list-checked"
                                            style="list-style: none; border: 1px solid #757575;margin: 1em 8em 1em 8em;">
                                            <li>3 years of Teaching Experience</li>
                                            <li>Experienced in teaching basics of coding to Grade 1 students</li>
                                            <li>Taught Database Management System,Operating system & Artificial
                                                Intelligence to Engineering students</li>

                                        </ul>

                                        <h5 class="text-center">Course Overview</h5>
                                        <ul class="list-checked"
                                            style="list-style: none; border: 1px solid #757575;margin: 1em 8em 1em 8em;">
                                            <li>Learn from Best Teachers from IITs & other top tier colleges with 10+
                                                years of Experience</li>
                                            <li>Delivering customized learning based on the student's learning pace</li>
                                            <li>Delivering customized learning based on the student's learning pace</li>

                                        </ul>

                                        <div class="text-center">
                                            <a href="<%= liveclass.link %>" target="blank" class="btn btn-default btn-rounded text-center">
                                                Enter to class Room </a>
                                        </div>


                                    </div>
                                </div>
                            </div>
                        </li>



                    </ul>
                </div>
            </div>
        </div>
    </div>
    <!-- contact area END -->
</div>
<!-- Content END-->