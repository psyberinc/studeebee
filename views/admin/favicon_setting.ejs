
	<!--Main container start -->
	<main class="ttr-wrapper">
		<div class="container-fluid">
			<div class="db-breadcrumb">
				<h4 class="breadcrumb-title">Add Favicon Logo</h4>
				<ul class="db-breadcrumb-list">
					<li><a href="#"><i class="fa fa-home"></i>Home</a></li>
					<li>Add Favicon Logo</li>
				</ul>
			</div>
			<div class="row">
				<!-- Your Profile Views Chart -->
				<div class="col-lg-12 m-b30">
					<div class="widget-box">
						<div class="widget-inner">
							<form class="edit-profile m-b30" action="/admin/favicon-setting" method="POST" type="file"
								enctype="multipart/form-data">
								<div class="row">
									

									<div class="form-group col-12">
										<span class="col-form-label">Insert Favicon image</span>
                                        <div class="col-form-label"><%= typeof msg != 'undefined' ? msg: '' %>
                                            <img src="/uploads/<%= setting.favicon %>"  style="height: 100px; width: 100px; text-align: center;" alt=""></div>
										<input class="form-control" name="favicon" type="file" required>
									</div>

									


									<div class="col-12">
										<button type="submit" class="btn-secondry add-item m-r5"><i
												class="fa fa-fw fa-plus-circle"></i>&nbsp;Publish</button>
									</div>
								</div>
							</form>
						</div>
					</div>
				</div>
				<!-- Your Profile Views Chart END-->
			</div>
		</div>
	</main>
	<div class="ttr-overlay"></div>
