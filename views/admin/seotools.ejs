
            <!--Main container start -->
            <main class="ttr-wrapper">
                <div class="container-fluid">
                    <div class="db-breadcrumb">
                        <h4 class="breadcrumb-title">SEO Tools</h4>
                        <ul class="db-breadcrumb-list">
                            <li><a href="#"><i class="fa fa-home"></i>Home</a></li>
                            <li>SEO Tool</li>
                        </ul>
                    </div>
                    <div class="row">
                        <!-- Your Profile Views Chart -->
                        <div class="col-lg-12 m-b30">
                            <div class="widget-box">
                                <div class="widget-inner">
                                    <form class="edit-profile m-b30" action="/admin/seo-tool" method="POST">
                                        <div class="row">
                                            <div class="form-group col-6">
                                                <label class="col-form-label">Site Title</label>
                                                <div>
                                                    <input class="form-control" type="text" value="<%= setting.siteTitle %>" name="siteTitle" required>
                                                </div>
                                            </div>

                                            <div class="form-group col-6">
                                                <label class="col-form-label">Site Name</label>
                                                <div>
                                                    <input class="form-control" type="text" value="<%= setting.siteName %>" name="siteName" required>
                                                </div>
                                            </div>
                                            <div class="seperator"></div>

                                            <div class="form-group col-12">
                                                <label class="col-form-label">Site Description</label>
                                                <div>
                                                    <textarea class="form-control" name="siteDescription"><%= setting.siteDescription %> </textarea>
                                                </div>
                                            </div>
                                            <div class="form-group col-12">
                                                <label class="col-form-label">Tags/Keywords</label>
                                                <div>
                                                    <input class="form-control" type="text" value="" name="" placeholder="Separeate Each Tags with a space">
                                                </div>
                                            </div>
                                            <div class="form-group col-12">
                                                <label class="col-form-label">Google Analytics</label>
                                                <div>
                                                    <textarea class="form-control" style="height:50%;" placeholder="Google Analytics code" name="analyticsId"> <%= setting.analyticsId %></textarea>
                                                </div>
                                            </div>

                                            <div class="col-12">
                                                <button type="submit" class="btn-secondry add-item m-r5"><i
												class="fa fa-fw fa-plus-circle"></i>&nbsp; Save Changes</button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                        <!-- Your Profile Views Chart END-->
                    </div>
                </div>
            </main>
            <div class="ttr-overlay"></div>
