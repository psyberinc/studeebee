	<!--Main container start -->
	<main class="ttr-wrapper">
		<div class="container-fluid">
			<div class="db-breadcrumb">
				<h4 class="breadcrumb-title">Instructor</h4>
				<ul class="db-breadcrumb-list">
					<li><a href="#"><i class="fa fa-home"></i>Home</a></li>
					<li>Instructor</li>
				</ul>
			</div>
			<div class="row">
				<!-- Your Profile Views Chart -->
				
				<div class="col-lg-12 m-b30">
					<div class="widget-box">
						<div class="widget-inner">
							<table class="table">
                                <thead class="thead-dark">
                                  <tr>
                                    <th scope="col">#</th>
                                    <th scope="col">Image</th>
                                    <th scope="col">User Name</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Full Name</th>
                                    <th scope="col">Phone No.</th> 
                                    <th scope="col">Action</th>
                                  </tr>
                                </thead>
                                <tbody>
                                    <% for( let i = 0; i < student.length; i++ ) { %>
                                  <tr>
                                    <th scope="row"><%= i+1 %> </th>
                                    <td><img height="100" width="100" src="/uploads/<%= student[i].image %>" class="img-thumbnail"></td>
                                    <td><%= student[i].username %> </td>
                                    <td><%= student[i].email %> </td>
                                    <td><%= student[i].fullName %> </td>
                                    <td><%= student[i].phone %> </td>
									<td><a href="/admin/view_instructor/<%= student[i].id %> " ><span style="padding-left: 5px;" class="fa fa-eye"></span></a>
										<a href="/admin/delete_instructors/<%= student[i].id %> " ><span style="padding-left: 5px;" class="fa fa-trash"></span></a></td>
                                  </tr>                                
                                </tbody>
                                <% } %>
                              </table>
						</div>
					</div>
				</div>
				<!-- Your Profile Views Chart END-->
			</div>
		</div>
	</main>


	<div class="ttr-overlay"></div>
