	<!--Main container start -->
	<main class="ttr-wrapper">
		<div class="container-fluid">
			<div class="db-breadcrumb">
				<h4 class="breadcrumb-title">Modules</h4>
				<ul class="db-breadcrumb-list">
					<li><a href="#"><i class="fa fa-home"></i>Home</a></li>
					<li>Modules</li>
				</ul>
			</div>
			<div class="row">
				<!-- Your Profile Views Chart -->
				<a  href="/admin/add-course-video/<%= courseid %>/<%= index %>" class="btn">Add Video</a>
				<div class="col-lg-12 m-b30">
					<div class="widget-box">
						<div class="widget-inner">
							<table class="table">
                                <thead class="thead-dark">
                                  <tr>
                                    <th scope="col">#</th>
                                    <th scope="col">Module Name</th>
                                    
                                    <th scope="col">Video Title</th>
                                    <th scope="col">Video Link</th>
                                    <th scope="col">Duration</th>
                                    
                                    <th scope="col">Action</th>
                                  </tr>
                                </thead>
                                <tbody>
                                    <% for( let i = 0; i < course.sectionVideoTitle.length; i++ ) { %>
                                  <tr>
                                    <th scope="row"><%= i+1 %> </th>
                                    <td><%= course.sectionTitle %> </td>
                                    <td><%= course.sectionVideoTitle[i] %> </td>
                                    <td><%= course.sectionVideoUrl[i] %> </td>
                                    <td><%= course.videoDuration[i] %> </td>
									<td><a href="/admin/edit-module-video/<%= courseid %>/<%= index %>/<%= i %> "><span class="fa fa-pencil-square-o"></span></a> <a href="/admin/delete-module-video/<%= courseid %>/<%= index %>/<%= i %> " ><span style="padding-left: 5px;" class="fa fa-trash"></span></a></td>
                                  </tr>                                
                                </tbody>
                                <% } %>
                              </table>
						</div>
					</div>
				</div>
				<!-- Your Profile Views Chart END-->
			</div>
		</div>
	</main>


	<div class="ttr-overlay"></div>
